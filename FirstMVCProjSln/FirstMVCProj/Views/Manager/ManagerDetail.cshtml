@{
}
@model IEnumerable<FirstMVCProj.Models.Manager>

    <table class="table">
        <tr>
            <th>Manager Id</th>
            <th>Manager Name</th>
            <th>Salary</th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
        @foreach(var item in Model)
        {
          <tr>
              <td>@item.Manager_Id</td>
              <td>@item.Manager_Name</td>
              <td>@item.Salary</td>
              <td><a asp-action="Details" asp-route-id="@item.Manager_Id"> Know More</a></td>
              <td><a asp-action="Edit" asp-route-id="@item.Manager_Id"> Edit</a></td>
              <td><a asp-action="Delete" asp-route-id="@item.Manager_Id"> Delete</a></td>
          </tr>
        }

    </table>
